package strings;

public class Abecedario {

	public static void main(String[] args) {
		
		for(int i=65; i<91; i++)
			System.out.print((char)i+" ");		//Abecedario en mayusculas
		
		System.out.println();
		
		for(int i=97; i<123; i++)
			System.out.print((char)i+" ");		//Abecedario en minusculas

	}

}
