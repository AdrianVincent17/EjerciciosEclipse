package strings.ejemplos;

public class LocalizarEspaciov2 {

	public static void main(String[] args) {

		//Localizar el espacio con el indexOff

		String nombre="Harry Potter";
		char espacio=' ';

		if(nombre.indexOf(espacio)==-1)
			System.out.println("No tiene espacios");
		else 
			System.out.println("Tiene un espacio");

	}

}
