package matrices;

public class ListadoClase {
	
	public static int aleatorio(int num) {
		return (int)(Math.random()*num+1);
	}
	
	final static int FILAS=4;
	final static int COLUMNAS=6;

	public static void main(String[] args) {

		String [] clase= {"Jesus C.", "Alberto C.", "Marlon C.","Hamza D.","Adam D.",
						  "Jose G.", "Angel G.", "Mehdi L.","Joaquin L", "Rafael L.",
						  "Fran L.","Elena L.","Uriel L.", "Alvaro L.", "Uriel M.", 
						  "Felix R.","JAPe√±alver", "Jesus P.", "Erik P.","Eduardo P.", 
						  "Ivan S.", "Jalil T.", "Alejandro","Adrian V."}; 
		
		int [][] matrizclase=new int [FILAS][COLUMNAS];
		
		int puesto=1;
		System.out.println("\tDistribucion de puestos");
		
		for(int i=0; i<matrizclase.length; i++) {
			for(int j=0; j<matrizclase[i].length; j++) {
					System.out.print(puesto+"\t");
					puesto++;
			}
			System.out.println(); 
		}
		
		
		
		
		for(int i=0; i<clase.length; i++) {
			System.out.println((i+1)+". "+clase[i]);
		}
		
		
		
	}

}
