package arrays;

public class Ejercicio08 {

	public static void imprimir(int[] array) {

		for(int i=0; i<array.length; i++) 
			System.out.print(array[i]+" ");
			System.out.println();
		
	}

	public static void main(String[] args) {
		
		int a[]= {1,3,5,7,9};
		int[] b= {2,4,6,8,10};
		
		System.out.print("a: ");
		imprimir(a);
		System.out.print("b: ");
		imprimir(b);


	}

}
